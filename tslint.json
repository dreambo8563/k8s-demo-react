{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
    "semicolon": [true, "never"],
    "interface-name": [true, "never-prefix"],
    "no-empty-interface": false,
    "trailing-comma": false,
    "arrow-parens": false,
    "object-literal-sort-keys": false,
    "no-console": [false, "log", "error"]
  },
  "rulesDirectory": ["src"]
}
